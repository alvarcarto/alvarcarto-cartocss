Map {
  background-color: @land-color;
}

@land-color: #222;
// Full white will render transparent in plywood, hence the name
@water-color: #fff;

@glacier: @water-color;
@glacier-line: darken(@water-color, 8%);

@admin-boundaries: #fff;

@building-fill: #fff;
@building-aeroway-fill: #fff;
@bridge-color: @land-color;
